{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
   
    "rules": {
        "react/function-component-definition": "off",
        "linebreak-style": "off",
        "react/js-indent": 0,
        "no-trailing-spaces": "off",
        "arrow-body-style": "off",
        "import/no-cycle": "off"
    }
}
